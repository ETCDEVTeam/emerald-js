    "@material-ui/core": "^1.3.0",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3.0.8",
    "prop-types": "^15.6.0",
    "qs": "^6.5.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-jss": "^8.1.0",
    "react-required-if": "^1.0.3"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
